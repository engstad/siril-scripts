requires 1.3.4

# Convert Flat Frames to .fit files
cd Flat/Clear
convert flats-clear -out=../../process/flats-clear
cd ../../process/flats-clear

# Calibrate Flat Frames
calibrate flats-clear -bias="=500"
# "=10*$OFFSET" 

# Stack Flat Frames to pp_flat_stacked.fit
stack pp_flats-clear rej 3 3 -norm=mul -out=../../flats-clear-stacked
cd ../..
